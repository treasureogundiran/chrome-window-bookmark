{
    "manifest_version": 3,
    "name": "Window Bookmark",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "img/bookmark-website.png"
    },
    "permissions": [
        "tabs"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
  }